/// <summary>
/// A factory that creates empty binary search tree tables from a value-key mapping function.
/// </summary>
public class BinaryTreeTableFactory<TKey, TValue> : IFactory<ITable<TKey, TValue>, IMap<TValue, TKey>>
{
    public ITable<TKey, TValue> Create(IMap<TValue, TKey> Argument)
    {
        return new TreeTable<TKey, TValue>(new BinarySearchTree<TValue, TKey>(Argument));
    }
}