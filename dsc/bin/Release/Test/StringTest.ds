public class StringTest
{
    public const this(set int32 val);
    
    private int32 val;
    public int32 Value { const get return val; set val = value; }
    public char CharVal { const get return (char)val; set val = (int32)value; }
    
    public void SetToA()
    {
        CharVal = '\\';
    }

    public int StringLength { const get return ((string)CharVal).Length; }

    public override const string ToString()
    {
        return (string)(char)this.val;
    }
    public const float32 ToFloat32()
    {
        return (float32)(bit32)val;
    }
    public const bool EndsWith(string Value)
    {
        return ToString().EndsWith(Value);
    }
}