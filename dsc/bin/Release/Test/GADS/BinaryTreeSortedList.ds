/// <summary>
/// A sorted list that uses a binary tree.
/// </summary>
public class BinaryTreeSortedList<TRecord, TKey> : ISortedList<TRecord>
{
    private BinaryTree<KeyValuePair<TKey, TValue>> tree;

    /// <summary>
    /// Gets the mapping function that maps the records to their respective keys.
    /// </summary>
    public IMap<TRecord, TKey> KeyMap { const get; private set; }
}