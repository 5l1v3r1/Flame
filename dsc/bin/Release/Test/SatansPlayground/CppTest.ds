using System;

public class TestClass
{
    public const this();
    public const this(set int Value);

    private int Value;

    public const int GetValue() 
        return Value;

    public TestClass GetThis() return this;

    public const long Pow(int Exponent)
    {
        long result = 1;
        for (int i = 0; i < Exponent; i++)
            result *= Value;
        return result;
    }

    public const TestClass Pow(TestClass Exponent)
    {
        TestClass result = new TestClass(1);
        for (int i = 0; i < Exponent.Value; i++)
            result *= Value;
        return result;
    }

    public const TestClass operator +(TestClass Other)
    {
        return new TestClass(Value + Other.Value);
    }
    public const TestClass operator *(TestClass Other)
    {
        return new TestClass(Value * Other.Value);
    }
}