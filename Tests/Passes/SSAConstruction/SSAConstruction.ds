
public class TestClass
{
    public int Select(bool Condition, int Left, int Right)
    {
        int result;
        if (Condition)
            result = Left;
        else
            result = Right;
        return result;
    }

    public int Loop(int Target)
    {
        int result = 0;
        while (result < Target)
            result++;
        return result;
    }
}
