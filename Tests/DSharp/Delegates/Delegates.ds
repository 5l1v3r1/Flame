using System;
using System.Collections.Generic;

public static class DelegatesTest
{
    public static const int Twice(int Value)
    {
        return Value * 2;
    }
    public static const double Twice(double Value)
    {
        return Value * 2.0;
    }

    public static const [T] Apply<T>([T] Values, T(T) Func)
    {
        var results = new List<T>();
        foreach (var item in Values)
        {
            results.Add(Func(item));
        }
        return results;
    }

    public static const [int] Double([int] Values)
    {
        return Apply<int>(Values, Twice);
    }
    public static const [double] Double([double] Values)
    {
        return Apply<double>(Values, Twice);
    }
}
