// A compare-test test description.

build
{
    $(dsc) $(Filename).ds -platform clr -o bin/$(Filename).exe $(Flags);
}

run
{
    return = @($(net-runtime) bin/$(Filename).exe $(Args));
}

configs
{
    Og { Flags = -Og; }
    O2 { Flags = -O2; }
    // O3 { Flags = -O3; }
}

init
{
    dsc = dsc;
    Filename = Exceptions;
    Args = 42;

    // Make sure that the output is "Oops?\nFinally!"
    return = @(echo "Oops?")@(echo "Finally!");
}
