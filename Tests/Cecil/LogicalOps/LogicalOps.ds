using System;
using System.Collections.Generic;
using System.Text;
using System.Linq;

public static class LogicalOpsTest
{
    public const bool LogicalAnd(bool First, bool Second)
    {
        return First && Second;
    }

    public const bool LogicalOr(bool First, bool Second)
    {
        return First || Second;
    }

    public static void Main(string[] Args)
    {
        Console.WriteLine(LogicalAnd(false, false));
        Console.WriteLine(LogicalAnd(false, true));
        Console.WriteLine(LogicalAnd(true, false));
        Console.WriteLine(LogicalAnd(true, true));
        Console.WriteLine(LogicalOr(false, false));
        Console.WriteLine(LogicalOr(false, true));
        Console.WriteLine(LogicalOr(true, false));
        Console.WriteLine(LogicalOr(true, true));
    }
}
