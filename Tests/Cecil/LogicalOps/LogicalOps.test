// A compare-test test description.

build
{
    $(dsc) $(Filename).ds -platform clr -o bin/$(Filename).exe $(Flags);
}

run
{
    return = @($(net-runtime) bin/$(Filename).exe);
}

configs
{
    Og { Flags = -Og; }
    O2 { Flags = -O2; }
    O3 { Flags = -O3; }
}

init
{
    Filename = LogicalOps;
    return = @($echo False)@($echo False)@($echo False)@($echo True)@($echo False)@($echo True)@($echo True)@($echo True);
}
