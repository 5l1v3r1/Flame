
public module spectest
{
    /// <summary>
    /// Prints a single character to standard output.
    /// </summary>
    [[import]]
    public void printChar(char Value);
}

public module Console
{
    public void Write(char Value)
    {
        spectest.printChar(Value);
    }

    public void WriteLine()
    {
        Write('\n');
    }
}

public module Program
{
    public void Main()
    {
        Console.Write('H');
        Console.Write('e');
        Console.Write('l');
        Console.Write('l');
        Console.Write('o');
        Console.Write(' ');
        Console.Write('w');
        Console.Write('o');
        Console.Write('r');
        Console.Write('l');
        Console.Write('d');
        Console.Write('!');
        Console.WriteLine();
    }
}
