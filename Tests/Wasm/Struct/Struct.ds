
public module spectest
{
    /// <summary>
    /// Prints a single character to standard output.
    /// </summary>
    [[import]]
    public void print(int Value);
}

public struct Vector2
{
    public int X;
    public int Y;

    public int LengthSquared()
    {
        return X * X + Y * Y;
    }
}

public module StructTest
{
    public Vector2 CopyVector(Vector2 Value)
    {
        return Value;
    }

    public int Allocate()
    {
        Vector2 val;
        val.X = 3;
        val.Y = 4;
        Vector2 copy = val;
        return CopyVector(copy).LengthSquared();
    }

    public void Main()
    {
        spectest.print(Allocate());
    }
}
