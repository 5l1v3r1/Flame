
public struct Vector2
{
    public int X;
    public int Y;

    public int LengthSquared()
    {
        return X * X + Y * Y;
    }
}

public module StructTest
{
    public int Allocate()
    {
        Vector2 val;
        val.X = 3;
        val.Y = 4;
        Vector2 copy = val;
        return copy.LengthSquared();
    }
}
