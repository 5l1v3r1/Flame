      <dependency id="Pixie" version="0.1.7" />
      <dependency id="Pixie.Loyc" version="0.1.7" />
      <dependency id="System.Collections.Immutable" version="1.5.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\README.md" target="readme.txt" />
    <file src="Flame.Ir\bin\Release\Flame.Ir.dll" target="lib\net45\" />
