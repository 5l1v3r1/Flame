using System;
using System.Collections.Generic;
using System.Text;

namespace Flame.Compiler.Expressions
{
    public abstract class ConditionalExpression : BinaryOperatorExpression
    {
    	protected const this();
    
        protected virtual override IType GetExpressionType()
        {
            if (OperatorOverload != null) return OperatorOverload.ReturnType;
			else return PrimitiveTypes.Boolean;
        }
    }
}