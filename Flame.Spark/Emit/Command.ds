using System;
using System.Collections.Generic;

namespace Flame.Spark.Emit
{
	public class Command : ICommand
	{
		public const this(set OpCode CommandOpCode);
	
		public OpCode CommandOpCode : ICommand.CommandOpCode { const get; private set; }
	
		public void Emit(ISparkEmitContext Context)
		{
			Context.Emit(CommandOpCode);
		}
	}
}