using System;
using System.Collections.Generic;

namespace Flame.Spark.Emit
{
	public class Bit32Command : ICommand
	{
		public const this(set OpCode CommandOpCode, set bit32 Data);
	
		public OpCode CommandOpCode : ICommand.CommandOpCode { const get; private set; }
		public bit32 Data { const get; private set; }
	
		public void Emit(ISparkEmitContext Context)
		{
			Context.Emit(CommandOpCode, Data);
		}
	}
}