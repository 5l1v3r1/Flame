using System;
using System.Collections.Generic;

namespace Flame
{
	public interface IConverter<in TSource, out TTarget>
	{
		TTarget Convert(TSource Value);
	}

	public class EmptyConverter<T> : IConverter<T, T>
	{
		public const this();

		public const T Convert(T Value)
		{
			return Value;
		}
	}
}
