using System;
using System.Collections.Generic;
using Flame.Build;

namespace Flame.Primitives
{
	public class EquatableType : MethodRootPrimitiveType
	{
		public const this();
	
		public override string Name { const get return "IEquatable"; }
		
		public override const IMethod[] GetMethods() 
		{
			return new IMethod[] { PrimitiveMethods.Equals };
		}
		
		public override bool Equals(object Other)
		{
			return Other is EquatableType;
		}
	}
}