using System;
using System.Collections.Generic;
using Flame.Build;

namespace Flame.Primitives
{
	public class HashCodeProviderType : MethodRootPrimitiveType
	{
		public const this();
	
		public override string Name { const get return "IHashCodeProvider"; }
		
		public override const IMethod[] GetMethods() 
		{
			return new IMethod[] { PrimitiveMethods.GetHashCode };
		}
		
		public override bool Equals(object Other)
		{
			return Other is HashCodeProviderType;
		}
	}
}